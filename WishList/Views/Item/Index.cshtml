@model List<WishList.Models.Item>
<h1>Wishlist</h1>
<a asp-action="create">Add Item</a>
<ul>
    @foreach (var item in Model)
    {
        <li>
            @item.Description
            <a asp-action="delete" asp-route-id=@item.Id>delete</a>

        </li>
    }
</ul>
<a asp-action="Index" asp-controller="Item">view wishlist</a>